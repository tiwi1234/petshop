            android:layout_height="@dimen/_170sdp"
           >

            <ImageView
                android:id="@+id/titik3"
                android:layout_width="@dimen/_40sdp"
                android:layout_height="wrap_content"
                android:background="@drawable/titik3"
                android:layout_marginTop="@dimen/_20sdp"
                android:layout_marginLeft="@dimen/_260sdp"
                />

            <ImageView
                android:id="@+id/btnkembali"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="wrap_content"
                android:background="@drawable/kembali"
                android:layout_marginTop="@dimen/_20sdp"
                android:layout_marginLeft="@dimen/_30sdp"
                />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_20sdp"
                android:layout_marginTop="@dimen/_20sdp"
                android:text="Profile"
                android:textColor="#495579"
                android:textSize="@dimen/_13sdp"
                android:textAlignment="center" />


        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/foto"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_120sdp"
            android:layout_marginTop="@dimen/_60sdp"
            >

            <ImageView
                android:layout_width="@dimen/_120sdp"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:background="@drawable/dedek" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/tulisan"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_80sdp"
